{
  "title": "home_end_one_hand",
  "rules": [
    {
      "description": "one handed home/end using fn+` fn+z, with shift support",
      "manipulators": [
        {
          "from": {
            "key_code": "grave_accent_and_tilde",
            "modifiers": {
              "mandatory": ["fn"]
            }
          },
          "to": [
            {
              "key_code": "home",
              "modifiers": []
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "z",
            "modifiers": {
              "mandatory": ["fn"]
            }
          },
          "to": [
            {
              "key_code": "end",
              "modifiers": []
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "grave_accent_and_tilde",
            "modifiers": {
              "mandatory": ["fn", "left_shift"]
            }
          },
          "to": [
            {
              "key_code": "home",
              "modifiers": ["left_shift"]
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "z",
            "modifiers": {
              "mandatory": ["fn", "left_shift"]
            }
          },
          "to": [
            {
              "key_code": "end",
              "modifiers": ["left_shift"]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "page up/down on fn+q fn+a",
      "manipulators": [
        {
          "from": {
            "key_code": "q",
            "modifiers": {
              "mandatory": ["fn"]
            }
          },
          "to": [
            {
              "key_code": "page_up",
              "modifiers": []
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "a",
            "modifiers": {
              "mandatory": ["fn"]
            }
          },
          "to": [
            {
              "key_code": "page_down",
              "modifiers": []
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}